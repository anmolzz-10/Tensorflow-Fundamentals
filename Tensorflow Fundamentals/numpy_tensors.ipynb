   ],
   "source": [
    "import numpy as np \n",
    "numpy_A=np.arange(1,25,dtype=np.int32) #Create a numpy array between 1 and 25\n",
    "\n",
    "tf.constant(numpy_A)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "1f915070",
   "metadata": {},
   "outputs": [],
   "source": [
    "A=tf.constant(value=numpy_A, dtype=tf.int32, shape=(2,3,4))"
   ]
